{
  "manifest_version": 2,
  "name": "tabs plan",
  "version": "1.0",
  "description": "manage tabs and plan and etc.!",
  "author": "CaoChang <crowcc@foxmail.com>",
  "default_locale": "en",
  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "img/icon16.png",
      "32": "img/icon32.png",
      "48": "img/icon48.png",
      "128": "img/icon128.png"
    }
  },
  "permissions": [
    "contextMenus",
    "storage",
    "tabs",
    "history",
    "identity",
    "notifications",
    "identity",
    "identity.email",
    "declarativeContent"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "commands": {
    "SAVEONE": {
      "suggested_key": {
        "default": "Ctrl+Shift+S",
        "mac": "Command+Shift+S"
      },
      "description": "save current tab"
    },
    "SAVECURRENTALL": {
      "suggested_key": {
        "default": "Ctrl+Shift+1",
        "mac": "Command+Shift+1"
      },
      "description": "save tabs in current window"
    },
    "SAVEALL": {
      "suggested_key": {
        "default": "Ctrl+Shift+2",
        "mac": "Command+Shift+2"
      },
      "description": "save all tabs"
    }
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
